@model IEnumerable<ff.Models.evenement>



<script>
    document.addEventListener("DOMContentLoaded", function () {
        var stripe = stripe("pk_test_51KZPaMLleOzOPgdyroHBGf0HJXZ8d0ZblguYEL8lL9RfVRxISmo41lpdGgm192JLv6ETRTaahoKcJb1peZupIRrF00mNEC7qnf");
        var elements = stripe.elements();
        var cardElement = elements.Create('card');
        cardElement.mount('#card-element');
    });
</script>

<p>
    Tous les evenements:
</p>

@foreach (var item in Model.Where(c => c.personneEmail != Session["e"].ToString()))
{

    <div class="listeParticipant">
        <table>

            <tr>
                <td>@Html.DisplayNameFor(model => model.dateDebut)</td>
                <td>@Html.DisplayFor(modelItem => item.dateDebut)</td>
            </tr>
            <tr>
                <td>@Html.DisplayNameFor(model => model.dateFin)</td>
                <td>@Html.DisplayFor(modelItem => item.dateFin)</td>
            </tr>
            <tr>
                <td>@Html.DisplayNameFor(model => model.adresse)</td>
                <td>@Html.DisplayFor(modelItem => item.adresse),@Html.DisplayFor(modelItem => item.ville), @Html.DisplayFor(modelItem => item.codePostal),@Html.DisplayFor(modelItem => item.pays)</td>
            </tr>
            <tr>
                <td>@Html.DisplayNameFor(model => model.sport)</td>
                <td>@Html.DisplayFor(modelItem => item.sport)</td>
            </tr>
            <tr>
                <td>@Html.DisplayNameFor(model => model.prix)</td>
                <td>@Html.DisplayFor(modelItem => item.prix)</td>
            </tr>
            <tr>
                <td>@Html.DisplayNameFor(model => model.nombreParticipant) / @Html.DisplayNameFor(model => model.nombreRejoint)</td>
                <td>@Html.DisplayFor(modelItem => item.nombreParticipant) / @Html.DisplayFor(modelItem => item.nombreRejoint)</td>
            </tr>

            <tr>
                <td>@Html.DisplayNameFor(model => model.heureDebut)</td>
                <td>@Html.DisplayFor(modelItem => item.heureDebut)</td>
            </tr>
            <tr>
                <td>@Html.DisplayNameFor(model => model.heureFin)</td>
                <td>@Html.DisplayFor(modelItem => item.heureFin)</td>
            </tr>
            <tr>
                <td>@Html.DisplayNameFor(model => model.type)</td>
                <td>@Html.DisplayFor(modelItem => item.type)</td>
            </tr>
            <tr>
                <td>@Html.DisplayNameFor(model => model.genre)</td>
                <td>@Html.DisplayFor(modelItem => item.genre)</td>
            </tr>
            <tr>
                <td>@Html.DisplayNameFor(model => model.dateCreation)</td>
                <td>@Html.DisplayFor(modelItem => item.dateCreation)</td>
            </tr>
            <tr>
                <td>@Html.DisplayNameFor(model => model.personneEmail)</td>
                <td>@Html.DisplayFor(modelItem => item.personneEmail)</td>
            </tr>
 
        </table>
    </div>

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <label>@ViewBag.show</label>
        </div>
    </div>

}

