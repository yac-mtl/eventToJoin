@model IEnumerable<ff.Models.personne>

@{
    ViewBag.Title = "Index";
}

<h2>Liste de tous les utilisateurs</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
