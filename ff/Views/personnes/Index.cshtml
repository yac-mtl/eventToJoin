        @Html.DisplayFor(modelItem => item.prenom)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.motDePasse)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.dateDeNaissance)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.genre)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.occupation)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.DateDeCreation)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.email)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.email }) |
        @Html.ActionLink("Details", "Details", "administrateurs", new { id = item.email }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.email })
      
